{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "LoggerBasePath": "c:\\logs\\adopetAPI",
  "LoggerFileTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.ffff}][{Level}] - AdopetAPI - {Message}{NewLine}{Exception}",
  "Serilog": {
    "Using": [ "Serilog.Skins.Console", "Serilog.Skins.File" ],
    "MinimumLevel": "Information",
    "WriteTo": [ "Console", "File" ]
  },
  "AllowedHosts": "*"
}
